    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "outDir": "../build"
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
